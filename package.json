{
  "name": "cleaning",
  "version": "1.0.0",
  "description": "angelus",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn ./src/app.ts",
    "build": "rimraf dist && tsc",
    "start": "node ./dist/app.js",
    "prepare": "husky install",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc --noEmit",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "test": "mocha --require ts-node/register 'src/**/*.test.ts' --timeout 20000 --exit",
    "commit": "git-cz",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/orm/config/ormconfig.ts",
    "typeorm:generate-migration": "npm run typeorm -- migration:generate  -d ./src/orm/config/ormconfig.ts ./src/orm/migrations/%npm_config_name%",
    "typeorm:show-migration": "npm run typeorm -- migration:show -d  ./src/orm/config/ormconfig.ts",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/orm/migrations/%npm_config_name%",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/orm/config/ormconfig.ts migration:revert",
    "seed:run": "npm run typeorm migration:run -- -d ./src/orm/config/ormconfig-seed.ts",
    "seed:run:dev": "npm run typeorm migration:run -- -d ./src/orm/config/ormconfig-seed.ts",
    "seed:create": "npm run typeorm -- migration:create ./src/orm/seeds/%npm_config_name%"
  },
  "dependencies": {
    "aws-sdk": "^2.1469.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^9.0.2",
    "mjml": "^4.14.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.5",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/multer-s3": "^3.0.1",
    "@types/node": "^17.0.33",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.3.6",
    "commitizen": "^4.2.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-no-array-reduce": "^1.0.58",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsc-files": "^1.1.3",
    "typescript": "^5.1.6"
  }
}
